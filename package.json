{
    "name": "0725-backend",
    "version": "0.0.1",
    "description": "",
    "author": "yulmwu",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli",
        "migration:generate": "npm run typeorm -- migration:generate src/migrations/Init --dataSource=typeorm.config.ts",
        "migration:run": "npm run typeorm -- migration:run --dataSource=typeorm.config.ts",
        "migration:revert": "npm run typeorm -- migration:revert --dataSource=typeorm.config.ts",
        "migration:rm": "rm -rf src/migrations/*",
        "migration:init": "npm run migration:rm && npm run migration:generate && npm run migration:run",
        "prettier": "prettier --write \"src/**/*.ts\""
    },
    "dependencies": {
        "@golevelup/nestjs-rabbitmq": "^6.0.2",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.1",
        "@nestjs/mapped-types": "*",
        "@nestjs/microservices": "^11.1.6",
        "@nestjs/platform-express": "^11.0.1",
        "@types/passport-local": "^1.0.38",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.8",
        "pg": "^8.16.3",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/amqplib": "^0.10.7",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.7",
        "prettier": "^3.6.2",
        "source-map-support": "^0.5.21",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3"
    }
}
